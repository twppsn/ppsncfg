<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:sys="clr-namespace:System;assembly=mscorlib">
	
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="Default.xaml" />
	</ResourceDictionary.MergedDictionaries>
	
	<!--Theme British racing Green-->
	<Color x:Key="PPSnDesktopColor">#004225</Color>
	<Color x:Key="PPSnAccentColor">#FFFFFF</Color>
	<Color x:Key="PPSnMarkerColor">#D2C62B</Color>
	<Color x:Key="PPSnControlBackgroundColor">#18543A</Color>
	<Color x:Key="PPSnActionButtonColor">#18543A</Color>
	<Color x:Key="PPSnWindowFooterColor">#637C36</Color>
	<Color x:Key="PPSnBlackColor">#000</Color>

	<sys:Single x:Key="Alternation0Transpareny">0.175</sys:Single>
	<sys:Single x:Key="Alternation1Transpareny">0.098</sys:Single>
	<sys:Single x:Key="SideBarTranspareny">0.05</sys:Single>

</ResourceDictionary>